<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <ImageView
            android:id="@+id/imageViewdarkblue"
            android:layout_width="430dp"
            android:layout_height="889dp"
            android:src="@drawable/ellipse_dark_blue"
            tools:ignore="MissingConstraints" />
        <ImageView
            android:id="@+id/imageViewlightblue"
            android:layout_width="430dp"
            android:layout_height="889dp"
            android:src="@drawable/ellipse_light_blue"
            tools:ignore="MissingConstraints" />
        <TextView
            android:id="@+id/Hello"
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:layout_marginLeft="90dp"
            android:layout_marginTop="25dp"
            android:fontFamily="arial"
            android:text="Your Slots!"
            android:textColor="@color/black"
            android:textFontWeight="600"
            android:textSize="40sp"
            android:translationZ="100dp" />
        <!-- 5 minute button start-->
        <ImageView
            android:layout_width="130dp"
            android:layout_height="130dp"
            android:layout_marginLeft="30dp"
            android:translationZ="10dp"
            android:layout_marginTop="90dp"
            android:src="@drawable/clock"/>
        <Button
            android:id="@+id/five_minute"
            android:layout_width="260dp"
            android:layout_height="100dp"
            android:layout_marginLeft="100dp"
            android:layout_marginTop="100dp"
            android:backgroundTint="@color/button_background_color"
            />
        <TextView
            android:id="@+id/five_minutes"
            android:layout_width="200dp"
            android:layout_height="100dp"
            android:text="2-5 Minutes"
            android:textSize="30sp"
            android:translationZ="100dp"
            android:layout_marginLeft="165dp"
            android:layout_marginTop="125dp"
            android:fontFamily="arial"
            android:textColor="@color/black"
            android:textFontWeight="1000"/>
        <!-- 10 minute button start-->
        <TextView
            android:id="@+id/ten_minutes"
            android:layout_width="220dp"
            android:layout_height="100dp"
            android:text="5-10 Minutes"
            android:textSize="30sp"
            android:translationZ="100dp"
            android:layout_marginLeft="165dp"
            android:layout_marginTop="310dp"
            android:fontFamily="arial"
            android:textColor="@color/black"
            android:textFontWeight="1000"/>
        <ImageView
            android:layout_width="130dp"
            android:layout_height="130dp"
            android:layout_marginLeft="30dp"
            android:translationZ="10dp"
            android:layout_marginTop="270dp"
            android:src="@drawable/clock"/>
        <Button
            android:id="@+id/ten_minute"
            android:layout_width="260dp"
            android:layout_height="100dp"
            android:layout_marginLeft="100dp"
            android:layout_marginTop="280dp"
            android:backgroundTint="@color/button_background_color"
            />


        <!-- 15 minute button start-->
        <TextView
            android:id="@+id/fifteen_minutes"
            android:layout_width="220dp"
            android:layout_height="100dp"
            android:text="15-30 Minutes"
            android:textSize="29sp"
            android:translationZ="100dp"
            android:layout_marginLeft="165dp"
            android:layout_marginTop="490dp"
            android:fontFamily="arial"
            android:textColor="@color/black"
            android:textFontWeight="1000"/>
        <ImageView
            android:layout_width="130dp"
            android:layout_height="130dp"
            android:layout_marginLeft="30dp"
            android:translationZ="10dp"
            android:layout_marginTop="450dp"
            android:src="@drawable/clock"/>
        <Button
            android:id="@+id/fifteen_minute"
            android:layout_width="260dp"
            android:layout_height="100dp"
            android:layout_marginLeft="100dp"
            android:layout_marginTop="455dp"
            android:backgroundTint="@color/button_background_color"
            />
    </RelativeLayout>
    </LinearLayout>